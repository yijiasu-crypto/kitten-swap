{
  "compilerOptions": {
    "lib": [
      "ES2018"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "target": "ES2018",
    "sourceMap": true,
    "esModuleInterop": true,
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "noEmitOnError": false,
    "types": ["node", "truffle-contracts"]
  }
}